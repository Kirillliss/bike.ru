sortRegions = ->
  options = $("#select_region option")
  arr = options.map((_, o) ->
    t: $(o).text()
    v: o.value
  ).get()
  arr.sort (o1, o2) ->
    (if o1.t > o2.t then 1 else (if o1.t < o2.t then -1 else 0))
  options.each (i, o) ->
    console.log i
    o.value = arr[i].v
    $(o).text arr[i].t
    return

